<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/txt_objective"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ll_subjective"
                android:layout_marginTop="15dp"
                android:background="@color/HeaderBg"
                android:padding="10dp"
                android:text="Examination"
                android:textColor="@color/TextBlue"
                android:textSize="18sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/ll_objective"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_objective"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical"
                android:padding="15dp"
                android:visibility="visible" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/txt_vitals"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/HeaderBg"
                        android:padding="10dp"
                        android:text="@string/Vitals"
                        android:textColor="@color/TextBlue"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/ll_vitals"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical"
                    android:paddingLeft="15dp"
                    android:visibility="visible" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingLeft="15dp" >

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt7"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/BP"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_bpl"
                                android:layout_width="70dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt7"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="low"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_bph"
                                android:layout_width="70dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/edit_bpl"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="high"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_bph"
                                android:gravity="center_vertical"
                                android:text="@string/mmofhg"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt8"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/Pulse"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_pulse"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt8"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="puls"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_pulse"
                                android:gravity="center_vertical"
                                android:text="@string/beatspermin"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt9"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/RespiratoryRate"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_respiratory"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt9"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="rr"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_respiratory"
                                android:gravity="center_vertical"
                                android:text="@string/cyclespermin"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt10"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/Temprature"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_temp"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt10"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="temp"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_temp"
                                android:gravity="center_vertical"
                                android:text="degree Fahrenheit"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt_weight"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/Weight"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_weight"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt_weight"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="weig"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_weight"
                                android:gravity="center_vertical"
                                android:text="@string/kilogram"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt_height"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="@string/Height"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_height"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt_height"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:inputType="textMultiLine"
                                android:paddingLeft="10dp"
                                android:tag="heig"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_height"
                                android:gravity="center_vertical"
                                android:text="@string/centimeters"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt_waist"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="Waist"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_waist"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt_waist"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:paddingLeft="10dp"
                                android:singleLine="true"
                                android:tag="waist"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_waist"
                                android:gravity="center_vertical"
                                android:text="@string/centimeters"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp" >

                            <TextView
                                android:id="@+id/txt_bmi"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:gravity="center_vertical"
                                android:text="BMI"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/edit_bmi"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_toRightOf="@+id/txt_bmi"
                                android:background="@drawable/edit_text_disabled_bg"
                                android:gravity="center_vertical"
                                android:paddingLeft="10dp"
                                android:singleLine="true"
                                android:tag="bmi"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/txt_bmi_unit"
                                android:layout_width="150dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@id/edit_bmi"
                                android:gravity="center_vertical"
                                android:text="kg/m2"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/txt_note"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text="Note"
                            android:textColor="#000000"
                            android:textSize="16sp" />

                        <LinearLayout
                            android:id="@+id/ll_note"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:background="@drawable/edit_text_disabled_bg"
                            android:orientation="horizontal"
                            android:padding="5dp" >

                            <TextView
                                android:id="@+id/edit_note"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#ffffff"
                                android:gravity="start"
                                android:padding="5dp"
                                android:tag="note"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/txt"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="start"
                        android:text="Symptoms : "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/edit_symptoms"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/txt"
                        android:background="@drawable/edit_text_disabled_bg"
                        android:gravity="start"
                        android:inputType="textMultiLine"
                        android:padding="10dp"
                        android:tag="symp"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/txt_general_exam"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:background="@color/HeaderBg"
                        android:padding="10dp"
                        android:text="@string/GeneralExamination"
                        android:textColor="@color/TextBlue"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/ll_general_exam"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:paddingLeft="15dp"
                    android:visibility="visible" >

                    <TextView
                        android:id="@+id/edit_general_exam"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/edit_text_disabled_bg"
                        android:gravity="start"
                        android:inputType="textMultiLine"
                        android:padding="10dp"
                        android:tag="ge"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/txt1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:text="Systemic Examination (Inspection, Palpation, Percussion, Auscultation) : "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/edit_se"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/txt1"
                        android:background="@drawable/edit_text_disabled_bg"
                        android:gravity="start"
                        android:inputType="textMultiLine"
                        android:padding="10dp"
                        android:tag="se"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/txt2"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="start"
                        android:text="Note : "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/edit_ex_note"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/txt2"
                        android:background="@drawable/edit_text_disabled_bg"
                        android:gravity="start"
                        android:inputType="textMultiLine"
                        android:padding="10dp"
                        android:tag="note"
                        android:textColor="#000000"
                        android:textSize="16sp" />
                </RelativeLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/txt_close"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_margin="8dp"
                android:src="@drawable/close"
                android:textColor="#ffffff"
                android:textSize="16sp"
                android:textStyle="bold" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>